/**index.wxss**/
.td {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 12px;
  margin-top: 5px;
  margin-bottom: 5px;
}
.divLineX{
  background: #E4E6ED;
  width: 100%;
  height: 5rpx;
}
.container1{
  background:#c6e6f5;
}

.contain2{
  border: 2px solid #E4E6ED;
  border-radius: 2px;
}
.myButton{
  border: 2px solid #E4E6ED; 
  border-radius: 0px;  
  background-color: white;
  margin-top: 15px;
  font-size: 12px;
  margin-bottom: 10px;
}
.myButton::after{
  border: none; 
}
.room{ 
/*  width: 50% !important;
  float:left;*/
  border-radius: 0px;  
  position: absolute;
  left: 37px;
  background-color: white;
  border: 2px solid #E4E6ED; 
  font-size: 12px;
 }
 .time{ 
 /*width: 50% !important;*/
  /*float:right;*/
  border-radius: 0px;  
  position: absolute;
  right: 38px;
  background-color: white;
  border: 2px solid #E4E6ED; 
  font-size: 12px;
 }
.contain3{
  bottom: 0rpx; 
  position: fixed;
  width: 100%;
}
/*弹窗*/
/*父盒子*/
.father{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;/*使得father中的windows水平居中*/
}
.windows{
  display: flex;
  flex-direction: column;/*表示该容器的子元素flex-item从上到下排列*/
  width: 250px;
  height: 430px;
  border:  2px solid #E4E6ED; 
  border-radius: 10px;
  background-color: white;
  position: absolute;
  top: 60px;
  z-index: 999;
}
.lessonMeg{
  width: 100%;
  justify-content: center;
  display: flex;
  font-size: 15px;
  margin-top: 10px;
  margin-bottom: 5px;
}
/*课程名字*/
.lesson{
  width: 100%;
  justify-content: center;
  display: flex;
  font-size: 15px;
  margin-top: 55px;
  margin-bottom: 5px;
}
/*按钮*/
.click{
  margin-top: 30px;
}
.bt{
  margin: 15px;
  background-color: white;
  border: 2px solid #E4E6ED;
  font-size: 12px;
}
